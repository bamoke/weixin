/* pages/service/index/index.wxss */
.m-index-banner {
  width:100%;
}
.service-list {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.service-list .item {
  box-sizing: border-box;
  width: 50%;
  padding: 24rpx;
  text-align: center;
}

.service-list .item .thumb {
  width: 100%;
  height: 180rpx;
}

.service-list .item .title {
  font-size: 30rpx;
}

.service-list .item .desc {
  color: #9c9c9c;
}
