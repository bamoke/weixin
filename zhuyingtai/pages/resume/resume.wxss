.m-form-default .field-list{
  padding:0 24rpx;
  background:#fff;
  border-top:1px solid #f1f1f1;
  border-bottom:1px solid #f1f1f1;
  font-size:30rpx;
}
.m-form-default .form-group {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:30rpx 0;
  border-bottom:1px solid #f1f1f1;
}
.m-form-default .form-group:last-child {
  border:none;
}
.m-form-default .form-group .label {
  flex-shrink: 0;
  flex-grow: 0;
  width:120rpx;
  white-space: nowrap;
  color:#232323;
}
.m-form-default .form-group .val {
  padding-left:48rpx;
  width: 100%;
  flex-shrink: 1;
  flex-grow: 1;
  text-align: right;
}
.m-form-default .btn-box {
  border-top:1px solid #f1f1f1;
  padding:80rpx 0;
  background: #fff;
  text-align: center;
}
.m-form-default .btn-box .btn {
  display: inline-block;
  width:80%;
  border-radius:45rpx;
}