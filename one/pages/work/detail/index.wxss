/* pages/work/detail/index.wxss */
.g-start-page {
  padding:24rpx;
}
.g-start-page .title {
  margin-bottom:24rpx;
  font-size:32rpx;
  color:#121212;
}
.g-start-page .description {
  margin-bottom:40rpx;
}
.m-question-nu {
  padding: 24rpx;
}
.m-question-nu .util {
  display: inline-block;
  margin-right:12rpx;
  width:48rpx;
  height:48rpx;
  line-height: 48rpx;
  text-align: center;
  color:#ccc;
  border-radius:50%;
  border:1px solid #ddd;
}
.m-question-nu .active {
  background-color:#2a94fa;
  color:#fff;
  border-color:#2a94fa;
}
.m-question-nu .correct {
  background-color:#00cc66;
  color:#fff;
  border-color:#00cc66;
}
.m-question-nu .error {
  background-color:#f30;
  color:#fff;
  border-color:#f30;
}

